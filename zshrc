# Set ANT and JAVA path variables
export ANT_HOME="/home/jason/apache-ant/bin:/home/jason/apache-ant/"
export JAVA_HOME="/home/jason/java/jdk1.8.0_201"
export JAVA="/home/jason/java/jdk1.8.0_201/bin"
export ANT_OPTS="-Xms256M -Xmx512M"
export PATH="${JAVA_HOME}:$JAVA:${ANT_HOME}:$PATH"

# Function for getting to game ai
function ai() {
    if [ $1 ]; then 
        cd ~/Documents/Schoolwork/CS_Spring19/homework$1
    else 
        cd ~/Documents/Schoolwork/CS_Spring19
    fi
}

# Function for getting to ubicomp hw
function uhw() {
    cd ~/Documents/Schoolwork/muc-spring-2019-localization
}

# Stuff for random game ai project
function pcg() {
    java -cp bin dk.itu.mario.engine.PlayCustomized $1
}


# Aliases for getting to projects fast
alias rc="cd ~/Documents/robocup-software"
alias its="cd ~/Documents/IRS-v2"
alias ubi="cd ~/Documents/Schoolwork/Ubiquitous-Nations"

